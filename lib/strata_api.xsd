			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="recommendation">
		<xs:annotation>
			<xs:documentation>A suggestion referring to a solution and related meta data.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tns:trackedEntity">
					<xs:sequence>
						<xs:element name="algorithmScore" type="xs:double" />
						<xs:element name="analysisAlgorithm" type="xs:string" />
						<xs:element name="analysisAlgorithmVersion" type="xs:string" />
						<xs:element name="analysisCategory" type="xs:string" />
						<xs:element name="analysisModule" type="xs:string" />
						<xs:element name="analysisService" type="xs:string" />
						<xs:element name="analysisServiceVersion" type="xs:string" />
						<xs:element name="bucket" type="xs:string" />
						<xs:element name="caseNumber" type="xs:string" />
						<xs:element name="client" type="xs:string" />
						<xs:element name="clientVersion" type="xs:string" />
						<xs:element name="display" type="xs:boolean" />
                                                <xs:element name="analysisStatus" type="xs:string" />
						<xs:element name="firstSuggestedDate" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
						<xs:element name="keywords" type="xs:string" />
						<xs:element name="lastSuggestedDate" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
						<xs:element name="linked" type="xs:boolean" minOccurs="0" maxOccurs="1" />
						<xs:element name="linkedAt" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
						<xs:element name="linkedBy" type="xs:string" minOccurs="0" maxOccurs="1" />
						<xs:element name="location" type="xs:string" />
						<xs:element name="luceneScore" type="xs:double" />
						<xs:element name="message" type="xs:string" minOccurs="0" maxOccurs="1" />
						<xs:element name="note" type="xs:string" />
						<xs:element name="origin" type="xs:string" minOccurs="0" maxOccurs="1" />
						<xs:element name="pinnedAt" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
						<xs:element name="pinnedBy" type="xs:string" minOccurs="0" maxOccurs="1" />
						<xs:element name="relevanceScore" type="xs:int" minOccurs="0" maxOccurs="1" />
						<xs:element name="resource" type="xs:string" />
						<xs:element name="resourceId" type="xs:string" />
						<xs:element name="resourceType" type="xs:string" minOccurs="0" maxOccurs="1" />
						<xs:element name="resourceURI" type="xs:string" minOccurs="0" maxOccurs="1" />
						<xs:element name="resourceViewURI" type="xs:string" minOccurs="0" maxOccurs="1" />
						<xs:element name="rule" type="xs:string" />
						<xs:element name="ruleVersion" type="xs:string" />
						<xs:element name="scoringAlgorithmVersion" type="xs:double" />
						<xs:element name="solutionId" type="xs:string" />
						<xs:element name="solutionKcsState" type="xs:string" />
						<xs:element name="solutionOwnerSSOName" type="xs:string" />
						<xs:element name="solutionTitle" type="xs:string" />
						<xs:element name="solutionAbstract" type="xs:string" />
						<xs:element name="solutionUrl" type="xs:string" />
						<xs:element name="suggestedCount" type="xs:double" minOccurs="0" maxOccurs="1" />
						<xs:element name="suggestionRelevanceScore" type="xs:double" minOccurs="0" maxOccurs="1" />
						<xs:element name="title" type="xs:string" minOccurs="0" maxOccurs="1" />
						<xs:element name="tracebackUrl" type="xs:string" minOccurs="0" maxOccurs="1" />
						<xs:element name="solutionCaseCount" type="xs:int" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>
									The count of cases linked to this solution.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="language" type="xs:string">
							<xs:annotation>
								<xs:documentation>
                                  This field is set based on the first of the following that is not empty or null:
                                     the language field returned by drupal
                                     the language as detected by Calaveras
                                     'en'
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="detectedLanguage" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									The language of the suggestion detected by language-detection tool.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="setLanguage" type="xs:string">
							<xs:annotation>
								<xs:documentation>
									The language of the suggestion set by the author in drupal.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ModerationState" type="xs:string" />
						<xs:element name="hasPublishedVersion" type="xs:string" />
						<xs:element name="product" type="xs:string" />
						<xs:element name="indexedDate" type="xs:string" />
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

