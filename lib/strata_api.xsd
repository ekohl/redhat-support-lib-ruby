						<xs:documentation>The formal product brand name.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="tns:suggested_artifacts" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>A list of Artifacts.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
