<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<!-- This document is used by JAXB to generate Java classes at project build time. Most entities can be the top-level payload 
	of an REST API response or embedded in another entity. -->
<xs:schema targetNamespace="http://www.redhat.com/gss/strata" elementFormDefault="qualified" version="1.14.0-SNAPSHOT-SNAPSHOT"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:tns="http://www.redhat.com/gss/strata" xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" jaxb:version="2.0" jaxb:extensionBindingPrefixes="xjc">

	<xs:annotation>
		<xs:documentation>Red Hat Customer Portal API</xs:documentation>
		<xs:appinfo>
			<jaxb:globalBindings>
				<xjc:serializable uid="11" />
				<jaxb:javaType xmlType="xs:dateTime" name="java.util.Calendar" parseMethod="javax.xml.bind.DatatypeConverter.parseDateTime"
					printMethod="javax.xml.bind.DatatypeConverter.printDateTime" />
				<!-- Global binding for xs to java boolean to allow case insensitivity -->
				<jaxb:javaType xmlType="xs:boolean" name="boolean"
					parseMethod="com.redhat.gss.avalon.strata.rest.BooleanDatatypeConverter.parseBoolean" />
			</jaxb:globalBindings>
		</xs:appinfo>
	</xs:annotation>

	<xs:complexType name="strataEntity" abstract="true">
		<xs:annotation>
			<xs:documentation>The base type for all API entities.</xs:documentation>
		</xs:annotation>
	</xs:complexType>

	<xs:complexType name="trackedEntity" abstract="true">
		<xs:annotation>
			<xs:documentation>The base type for entities that maintain create and update information.
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tns:strataEntity">
				<xs:sequence>
					<xs:element name="createdBy" type="xs:string" minOccurs="0" maxOccurs="1" />
					<xs:element name="createdDate" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
					<xs:element name="lastModifiedBy" type="xs:string" minOccurs="0" maxOccurs="1" />
					<xs:element name="lastModifiedDate" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="base_link">
		<xs:annotation>
			<xs:documentation>ATOM link type.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="tns:strataEntity">
				<xs:sequence>
					<xs:element name="value" type="xs:string" minOccurs="0" maxOccurs="1" />
				</xs:sequence>
				<xs:attribute name="title" type="xs:string" use="optional" />
				<xs:attribute name="rel" type="xs:string" use="optional" />
				<xs:attribute name="href" type="xs:string" use="optional" />
				<xs:attribute name="type" type="xs:string" use="optional" />
				<!-- This is not standard and is for backwards compatibility only -->
				<xs:attribute name="uri" type="xs:string" use="optional" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:element name="problems">
		<xs:annotation>
			<xs:documentation>A list of problem reports.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="source" type="xs:string" minOccurs="0" maxOccurs="1" />
				<xs:element ref="tns:link" />
				<xs:element ref="tns:problem" />
			</xs:choice>
		</xs:complexType>
	</xs:element>

	<xs:element name="problem">
		<xs:annotation>
			<xs:documentation>A problem report.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="source" type="xs:string" minOccurs="0" maxOccurs="1" />
				<xs:element ref="tns:link" />
			</xs:choice>
		</xs:complexType>
	</xs:element>

	<xs:element name="solutions">
		<xs:annotation>
			<xs:documentation>A list of solutions.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="tns:solution" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="articles">
		<xs:annotation>
			<xs:documentation>A list of articles.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="tns:article" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="sqiRating">
		<xs:annotation>
			<xs:documentation>An SQI Rating for a KCS Solution.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tns:trackedEntity">
					<xs:sequence>

						<xs:element name="solution" minOccurs="1" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>A link to the Solution this sqiRating is applicable to.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:anyURI">
										<xs:attribute name="rel" type="xs:string" use="optional" default="solution" />
										<xs:attribute name="solutionId" type="xs:string" use="optional" />
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>

						<xs:element name="completeTitle" type="xs:integer" minOccurs="1" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Is the title complete?</xs:documentation>

							</xs:annotation>
						</xs:element>

						<xs:element name="adheresToContentStandard" type="xs:integer" minOccurs="1" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Solution adheres to current template, style, and formatting guidelines in the content
									standard?
								</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="accuratelyReflectsProblem" type="xs:integer" minOccurs="1" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Does the issue accurately reflect the customer's problem and context?</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="isUnique" type="xs:integer" minOccurs="1" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Is article unique?</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="clearAndConcise" type="xs:integer" minOccurs="1" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Is content clear and concise?</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="metadataSetCorrectly" type="xs:integer" minOccurs="1" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Is article metadata set correctly?</xs:documentation>
							</xs:annotation>
						</xs:element>

					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="solution">
		<xs:annotation>
			<xs:documentation>A KCS Solution. Some elements have subelements called "html" and "text". The "html" field
				is a "safe value" field. It is the text field containing HTML markup rendered from the markdown stored in
				"value". The "html" field is not editable. The "text" field is the editable field that may contain
				markdown.
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tns:trackedEntity">
					<xs:sequence>

						<xs:element name="id" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Solution identifier assigned by backing system.</xs:documentation>
							</xs:annotation>
						</xs:element>
						
						<xs:element name="tnid" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Translated Solution identifier assigned by backing system.</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="uri" type="xs:anyURI" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>RESTful URI for this solution resource.</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="uidName" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Author name.</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="view_uri" type="xs:anyURI" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Portal URI for this solution resource.</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="title" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>The title of the solution.</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="authorSSOName" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>The SSO username of the user that authored the solution.</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="lastModifiedBySSOName" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>The SSO username of the user that last modified the solution.</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="language" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>
                                  The language the solution is written in.  When a solution is retrieved from drupal,
                                  this field is the language as set by the author of the solution.  When a solution
                                  is retrieved from Calaveras, this is the language as set by drupal unless that is
                                  empty or null, in which case this is set by Calaveras from detectLanguage.  If
                                  language detection fails, it is set by Calaveras to be 'en'.
                                </xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="issue" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>A description of the issue the solution resolves.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="text" type="xs:string" />
									<xs:element name="html" type="xs:string" />
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="environment" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>A description of the environment the solution applies to.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="text" type="xs:string" />
									<xs:element name="html" type="xs:string" />
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="resolution" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>A description of the resolution proposed to resolve the issue.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="text" type="xs:string" />
									<xs:element name="html" type="xs:string" />
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="rootCause" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>A description of the underlying cause of the issue.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="text" type="xs:string" />
									<xs:element name="html" type="xs:string" />
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="internalDiagnosticSteps" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>A description of the diagnostic steps a technician should take to diagnose the issue.
								</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="text" type="xs:string" />
									<xs:element name="html" type="xs:string" />
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="externalDiagnosticSteps" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>A description of the diagnostic steps a customer should take to diagnose the issue.
								</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="summary" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Summary of the solution.</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="tags" type="xs:string" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Tags associated with the solution.</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="case" type="xs:anyURI" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>A case linked to this solution.</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="supportNeed" type="xs:anyURI" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>A support need linked to this solution.</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="tag" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>A description tag for the solution.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="text" type="xs:string" />
									<xs:element name="html" type="xs:string" />
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="published" type="xs:boolean" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Is the solution published?</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="hasPublishedRevision" type="xs:boolean" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Does a published revision of this solution exist?</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="isLocked" type="xs:boolean" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Is the solution locked?</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="lockedBy" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Who locked the solution?</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="lockedAt" type="xs:dateTime" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>When was the solution locked?</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="lockExpiresAt" type="xs:dateTime" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>When will the lock expire?</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="duplicateOf" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Links to duplicates of the solution</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string">
										<xs:attribute name="rel" type="xs:string" use="optional" default="solution" />
										<xs:attribute name="solutionId" type="xs:string" use="optional" />
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>

						<xs:element name="body" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>body of solution/article</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="text" type="xs:string" />
									<xs:element name="html" type="xs:string" />
								</xs:sequence>
							</xs:complexType>
						</xs:element>

						<xs:element name="kcsState" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>The state in the KCS lifecycle the solution is currently in. The
									possible states are "wip", "verified", and "unverified".
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="sbrs">
							<xs:annotation>
								<xs:documentation>SBRs for this solution resource.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="sbr" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="ModerationState" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>The moderation state the solution is currently in.</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="caseCount" type="xs:int" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>
									The count of cases linked to this solution. This only appears
									in search results and recommendations.
								</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="abstract" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>
									This is a short summary of the Solution suitable for display as part of search results. If a particular solution
									does not have this field set; then one is automatically created by Calaveras.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="extract" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>
									This field is only set when the solution is part of search results (or recommendations). This is an excerpt from
									the solution which contains the section(s) of the solution that best indicate why this solution matched the
									search query.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="explanation" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>
									Lucene search result explanation of why and how well this document matches the search query.
									This field is only set when the solution is part of search results (or recommendations).
									This field is only for debugging purposes. See 'extract' for a more generally useful field.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="detectedLanguage" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>
                                  Only for solutions returned from Calaveras.
                                  Language of the solution detected by Google's language-detection.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="setLanguage" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>
                                  Only for solutions returned from Calaveras.
                                  The language as set by the author in drupal.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="product" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>
									Product for which this solution is meant for.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="indexedDate" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>
									Time when the solution has been indexed.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<xs:element name="article">
		<xs:annotation>
			<xs:documentation>A KCS Article. Some elements have subelements called "html" and "text". The "html" field
				is "safe value" field. It is the text field rendered as HTML so there is no markup in the "html" field and
				it is not editable. The "text" field is an editable field that may contain HTML markup.
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tns:trackedEntity">
					<xs:sequence>

						<xs:element name="id" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>A unique identifier for the solution assigned by the backing system.</xs:documentation>
							</xs:annotation>
						</xs:element>
						
					 <xs:element name="tnid" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Translated Solution identifier assigned by backing system.</xs:documentation>
							</xs:annotation>
					</xs:element>

						<xs:element name="uri" type="xs:anyURI" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>RESTful URI for this solution.</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="uidName" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Author name.</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="view_uri" type="xs:anyURI" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Portal URI for this solution.</xs:documentation>
							</xs:annotation>
						</xs:element>

						<!-- BOOKMARK -->
						<xs:element name="title" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Title</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="authorSSOName" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>AuthorSSOName. This field is required by the model
									but the schema allows it to be omitted so
									that field-level updates are possible.
								</xs:documentation>


							</xs:annotation>
						</xs:element>
						<xs:element name="lastModifiedBySSOName" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>lastModifiedBySSOName. This field is required by the model
									but the schema allows it to be
									omitted so that field-level updates are possible.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="language" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>language of the node/solution. This field is required by the model
									but the schema allows it to
									be omitted so that field-level updates are possible.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="issue" minOccurs="0" maxOccurs="1">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="text" type="xs:string" />
									<xs:element name="html" type="xs:string" />
								</xs:sequence>
							</xs:complexType>

						</xs:element>
						<xs:element name="environment" minOccurs="0" maxOccurs="1">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="text" type="xs:string" />
									<xs:element name="html" type="xs:string" />
								</xs:sequence>

							</xs:complexType>
							<!-- <xs:annotation> <xs:documentation>Attributes of the environment that make this problem a problem. Valid values 
								are available from the values service. </xs:documentation> </xs:annotation> -->
						</xs:element>
						<xs:element name="resolution" minOccurs="0" maxOccurs="1">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="text" type="xs:string" />
									<xs:element name="html" type="xs:string" />
								</xs:sequence>

							</xs:complexType>

						</xs:element>
						<xs:element name="rootCause" minOccurs="0" maxOccurs="1">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="text" type="xs:string" />
									<xs:element name="html" type="xs:string" />
								</xs:sequence>
							</xs:complexType>

						</xs:element>
						<xs:element name="internalDiagnosticSteps" minOccurs="0" maxOccurs="1">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="text" type="xs:string" />
									<xs:element name="html" type="xs:string" />
								</xs:sequence>
							</xs:complexType>
							<!-- <xs:annotation> <xs:documentation>Internal summary of steps that are used in troubleshooting.</xs:documentation> 
								</xs:annotation> -->
						</xs:element>
						<xs:element name="articleBody" minOccurs="0" maxOccurs="1">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="text" type="xs:string" />
									<xs:element name="html" type="xs:string" />
								</xs:sequence>

							</xs:complexType>

						</xs:element>
						<xs:element name="externalDiagnosticSteps" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>External summary of steps that are used in troubleshooting.</xs:documentation>
							</xs:annotation>
						</xs:element>


						<!-- <xs:element name="containerId" type="xs:string" minOccurs="0" maxOccurs="1"/> -->
						<xs:element name="summary" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Short summary or the first n words from the contents</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="tags" type="xs:string" minOccurs="0" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Tags associated with the solution</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="case" type="xs:anyURI" minOccurs="0" maxOccurs="unbounded">
							<!-- <xs:attribute name="rel" type="xs:string" use="optional" default="case"/> <xs:attribute name="caseNumber" type="xs:string" 
								use="optional"/> -->
							<xs:annotation>
								<xs:documentation>A Case linked to this Solution.</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element name="tag" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="text" type="xs:string" />
									<xs:element name="html" type="xs:string" />
								</xs:sequence>
							</xs:complexType>


						</xs:element>

						<xs:element name="published" type="xs:boolean" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Is Solution published?</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="duplicateOf" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string">
										<xs:attribute name="rel" type="xs:string" use="optional" default="solution" />
										<xs:attribute name="solutionId" type="xs:string" use="optional" />
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
							<!-- <xs:annotation> <xs:documentation>A link to Duplicates of this Solution.</xs:documentation> </xs:annotation> -->
						</xs:element>

						<!-- <xs:element name="sqiRating" type="sqiRating" minOccurs="0" maxOccurs="1"> <xs:annotation> <xs:documentation>SQI 
							Rating</xs:documentation> </xs:annotation> </xs:element> -->

						<xs:element name="kcsState" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>KCS State</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="body" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>Text body of the article without markdown.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="explanation" type="xs:string" minOccurs="0" maxOccurs="1">
							<xs:annotation>
								<xs:documentation>
									Lucene search result explanation of why and how well this document matches the search query.
									This field is only set when the solution is part of search results (or recommendations).
									This field is only for debugging purposes. See 'extract' for a more generally useful field.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>

	<!-- Lightweight emulation of ATOM link -->
